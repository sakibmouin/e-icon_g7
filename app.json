{
	"expo": {
		"name": "StarNav",
		"scheme": "starnav",
		"slug": "starnav",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#12171F"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#12171F",
				"package": "com.g7.starnav"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"]
	}
}
